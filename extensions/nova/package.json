{
  "name": "@openclaw/nova",
  "version": "2026.2.1",
  "description": "OpenClaw Nova channel plugin (nova.amazon.com)",
  "type": "module",
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.14",
    "openclaw": "workspace:*"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "nova",
      "label": "Nova",
